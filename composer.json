{
    "name": "czim/laravel-repository",
    "description": "Repository for Laravel (inspired by and indebted to Bosnadev/Repositories)",
    "keywords": [
        "laravel",
        "repository",
        "repositories",
        "eloquent",
        "database",
        "criteria"
    ],
    "homepage": "https://github.com/czim/laravel-repository",
    "license": "MIT",
    "authors": [
        {
            "name": "Coen Zimmerman",
            "email": "coen@pixelindustries.com",
            "homepage": "http://www.pixelindustries.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.4.0",
        "illuminate/support": "5.1.*",
        "illuminate/database": "5.1.*",
        "myclabs/php-enum": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "orchestra/testbench": "3.1.*",
        "watson/rememberable": "dev-master",
        "dimsav/laravel-translatable": "~5.0",
        "lookitsatravis/listify": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Czim\\Repository\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Czim\\Repository\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
