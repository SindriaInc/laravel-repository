{
    "name": "sindria/laravel-repository",
    "description": "Repository for Laravel (fork of czim/laravel-repository)",
    "keywords": [
        "laravel",
        "repository",
        "repositories",
        "eloquent",
        "database",
        "criteria"
    ],
    "homepage": "https://github.com/SindriaInc/laravel-repository",
    "license": "MIT",
    "authors": [
        {
            "name": "Luca Pitzoi",
            "email": "luca.pitzoi@sindria.org",
            "homepage": "https://sindria.org",
            "role": "DevOps"
        }
    ],
    "require": {
        "php" : ">=7.1.3",
        "illuminate/support": "^6.0|^7.0|^7.3",
        "illuminate/database": "^6.0|^7.0|^7.3",
        "myclabs/php-enum": "^1.7"
    },
    "require-dev": {
        "phpunit/phpunit" : "^7.0|^8.0",
        "orchestra/testbench": "^4.0",
        "watson/rememberable": "^3.2|^4.0",
        "astrotomic/laravel-translatable": "^11.5",
        "czim/laravel-listify": "~1.1.0"
    },
    "autoload": {
        "psr-4": {
            "Czim\\Repository\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Czim\\Repository\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
